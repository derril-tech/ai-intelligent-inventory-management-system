# StockSense AI Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# Application
NEXT_PUBLIC_APP_NAME=StockSense AI
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_APP_ENVIRONMENT=development

# API Configuration
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000
NEXT_PUBLIC_API_VERSION=v1
NEXT_PUBLIC_WS_URL=ws://localhost:8000

# Authentication
NEXT_PUBLIC_AUTH_DOMAIN=auth.stocksense.ai
NEXT_PUBLIC_AUTH_CLIENT_ID=your-auth-client-id
NEXT_PUBLIC_AUTH_REDIRECT_URI=http://localhost:3000/auth/callback

# External Services
NEXT_PUBLIC_ANALYTICS_ID=your-analytics-id
NEXT_PUBLIC_SENTRY_DSN=your-sentry-dsn
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_SENTRY=true
NEXT_PUBLIC_ENABLE_PWA=false
NEXT_PUBLIC_ENABLE_DARK_MODE=true

# Development
NEXT_PUBLIC_ENABLE_MOCK_DATA=false
NEXT_PUBLIC_ENABLE_DEBUG_MODE=true
NEXT_PUBLIC_LOG_LEVEL=debug

# Production (set these in production environment)
# NEXT_PUBLIC_API_BASE_URL=https://api.stocksense.ai
# NEXT_PUBLIC_WS_URL=wss://api.stocksense.ai
# NEXT_PUBLIC_APP_ENVIRONMENT=production
# NEXT_PUBLIC_ENABLE_DEBUG_MODE=false
# NEXT_PUBLIC_LOG_LEVEL=info
